{
  "verses": [
    {
      "id": 1,
      "verse_key": "1:1",
      "text_uthmani": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "In the name of Allah, the Entirely Merciful, the Especially Merciful"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Dengan nama Allah Yang Maha Pengasih lagi Maha Penyayang"
        }
      ]
    },
    {
      "id": 2,
      "verse_key": "1:2",
      "text_uthmani": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "All praise is due to Allah, Lord of the worlds"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Segala puji bagi Allah, Tuhan semesta alam"
        }
      ]
    },
    {
      "id": 3,
      "verse_key": "1:3",
      "text_uthmani": "الرَّحْمَٰنِ الرَّحِيمِ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "The Entirely Merciful, the Especially Merciful"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Yang Maha Pengasih, Maha Penyayang"
        }
      ]
    },
    {
      "id": 4,
      "verse_key": "1:4",
      "text_uthmani": "مَالِكِ يَوْمِ الدِّينِ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "Sovereign of the Day of Recompense"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Pemilik hari pembalasan"
        }
      ]
    },
    {
      "id": 5,
      "verse_key": "1:5",
      "text_uthmani": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "It is You we worship and You we ask for help"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Hanya kepada-Mu kami menyembah dan hanya kepada-Mu kami mohon pertolongan"
        }
      ]
    },
    {
      "id": 6,
      "verse_key": "1:6",
      "text_uthmani": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "Guide us to the straight path"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Tunjukilah kami jalan yang lurus"
        }
      ]
    },
    {
      "id": 7,
      "verse_key": "1:7",
      "text_uthmani": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
      "chapter_id": 1,
      "juz_number": 1,
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "sajdah": false,
      "page_number": 1,
      "translations": [
        {
          "id": 131,
          "language_name": "english",
          "text": "The path of those upon whom You have bestowed favor, not of those who have evoked Your anger or of those who are astray"
        },
        {
          "id": 33,
          "language_name": "indonesian",
          "text": "Jalan orang-orang yang Engkau beri nikmat, bukan jalan mereka yang dimurkai, dan bukan pula jalan mereka yang sesat"
        }
      ]
    }
  ]
}
